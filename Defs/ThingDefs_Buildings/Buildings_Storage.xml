<?xml version="1.0" encoding="utf-8" ?>
<Buildings>

  <!-- Crate -->

	<ThingDef ParentName="FurnitureBase">
		<defName>Crate</defName>
		<ThingClass>Building_Storage</ThingClass>
		<label>crate</label>
		<description>A simple box for storing items.</description>
		<rotatable>true</rotatable>
		<fillPercent>0.25</fillPercent>
		<PathCost>40</PathCost>
		<CastEdgeShadows>true</CastEdgeShadows>
		<Passability>PassThroughOnly</Passability>
		<graphicData>
			<texPath>Storage/Crate</texPath>
			<graphicClass>Graphic_Single</graphicClass>
      <damageData>
        <rect>(0.05,0.05,0.9,0.9)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
		</graphicData>
		    <building>
      <preventDeterioration>true</preventDeterioration>
      <fixedStorageSettings>
        <filter>
          <categories>
            <li>Foods</li>
            <li>Manufactured</li>
            <li>ResourcesRaw</li>
          </categories>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Important</priority>
      </defaultStorageSettings>
	  </building>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>15</costStuffCount>
		<AltitudeLayer>Building</AltitudeLayer>
		<statBases>
			<MaxHitPoints>140</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<WorkToBuild>650</WorkToBuild>
      <Mass>10</Mass>
		</statBases>
		<comps>
			<li Class="CompProperties_RoomIdentifier">
				<roomStat>Impressiveness</roomStat>
			</li>
		</comps>
	<inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <designationCategory>Furniture</designationCategory>
    <surfaceType>Item</surfaceType>
	</ThingDef>

