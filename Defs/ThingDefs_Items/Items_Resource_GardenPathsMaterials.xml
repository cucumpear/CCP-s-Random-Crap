<Defs>

  <ThingDef Abstract="True" Name="OrganicProductBase" ParentName="ResourceBase">
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>10</DeteriorationRate>
			<Mass>0.03</Mass>
    </statBases>
    <tickerType>Rare</tickerType>
  </ThingDef>
  
  
  <ThingDef ParentName="ResourceBase">
    <defName>CrushedStone</defName>
    <label>crushed stone</label>
	<thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <description>Finely crushed stone for decoration and soil preparation.</description>
    <stackLimit>300</stackLimit>
    <graphicData>
      <texPath>GardenPaths/Items/CrushedStone</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>0.1</MarketValue>
      <Mass>0.014</Mass>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="OrganicProductBase">
